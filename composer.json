{
  "name": "pug-ct/collections",
  "type": "library",
  "description": "PHP Collections library that adds additional functionality on top of Doctrine collections.",
  "keywords": [
    "php",
    "collections",
    "array",
    "iterators"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Mirko Rapisarda",
      "email": "mirkorap16@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2 || ^8.0",
    "doctrine/collections": "^1.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.2",
    "vimeo/psalm": "^3.12",
    "captainhook/captainhook": "^5.3"
  },
  "autoload": {
    "psr-4": {
      "PugCT\\": "lib/PugCT"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PugCT\\Tests\\": "tests/PugCT"
    }
  }
}
